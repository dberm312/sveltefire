  "version": "0.5.1",
  "description": "This is a fork of the sveltefire package updated to svelte 5 and firebase 11",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "fire": "firebase emulators:start --only auth,firestore,database,storage",
    "fire_all": "firebase emulators:start ",
    "listen": "lsof -i :8081 && lsof -i :9000",
    "kill": "lsof -ti:8081,9000 | xargs kill -9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
